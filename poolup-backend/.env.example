# PoolUp Backend Environment Variables

# Google OAuth Configuration
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# Plaid Configuration (Sandbox)
PLAID_CLIENT_ID=your_plaid_client_id_here
PLAID_SECRET=your_plaid_sandbox_secret_here
PLAID_ENV=sandbox

# Stripe Configuration
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key_here
STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here

# Database
DATABASE_URL=./poolup.db

# Server Configuration
PORT=3000
BASE_URL=http://localhost:3000
NODE_ENV=development

# JWT Secret (for production auth)
JWT_SECRET=your_jwt_secret_here_make_it_long_and_random

# Webhook URLs
PLAID_WEBHOOK_URL=http://localhost:3000/api/plaid/webhook
STRIPE_WEBHOOK_URL=http://localhost:3000/api/stripe/webhook
